!module
class=cpua
!view
view=title
text=Qualcomm Thermal Control
@show
echo true
@show
!view
!view
view=subtitle
text=control Qualcomm thermal-engine
@show
echo true
@show
!view

!view
view=button
text=Disable thermal engine
root=true
onboot=true
@click
stop thermal-engine
echo @toast=Executed.
@click
@show
if [ `which thermal-engine` != "" ]
then
echo true
fi
@show
core=true
!view


!module
