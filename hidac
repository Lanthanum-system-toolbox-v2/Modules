!module
class=extra
!view
view=title
text=Qualcomm Snapdragon Hi-Fi Audio (HI-DAC) Configuration
@show
echo true
@show
!view

!view
view=subtitle
text=Configure HI-DAC status
@show
echo true
@show
!view

!view
view=switch
text=Enanle HI-DAC
@current
if [ `cat /sys/module/snd_soc_wcd9330/parameters/high_perf_mode` == 1 ]
then
echo true
fi
@current
@click
echo $switch > /sys/module/snd_soc_wcd9330/parameters/high_perf_mode
@click
root=true
core=true
@show
if [ -e /sys/module/snd_soc_wcd9330/parameters/high_perf_mode ]
then
echo true
fi
@show
!view

!view
view=switch
text=Enable HI-DAC
@current
if [ `cat /sys/module/snd_soc_msm8x16_wcd/parameters/high_perf_mode` == 1 ]
then
echo true
fi
@current
@click
echo $switch > /sys/module/snd_soc_msm8x16_wcd/parameters/high_perf_mode
@click
root=true
core=true
@show
if [ -e /sys/module/snd_soc_msm8x16_wcd/parameters/high_perf_mode ]
then
echo true
fi
@show
!view
!module