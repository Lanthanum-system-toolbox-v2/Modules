!module
class=cpua
!view
view=title
text=Qualcomm GPU
@show
echo true
@show
!view

!view
view=subtitle
text=Adjust how radical to utilizethe GPU
@show
echo true
@show
!view

!view
view=seekbar
text=《——more conservative\s\s\s\s\s\s\s\s\s\s\s\s\s\s\s\s\smore radical——》
@max
cat /sys/class/kgsl/kgsl-3d0/min_pwrlevel
@max
@show
if [ -e /sys/class/kgsl/kgsl-3d0/default_pwrlevel ]
then
echo true
fi
@show
@current
r=`cat /sys/class/kgsl/kgsl-3d0/default_pwrlevel`
let c=max-r
echo $c
@current
#此处千万不能去掉调用前的$ 即使它在bash下可以被去掉
@done
let r=$max-$seekbar
echo $r > /sys/class/kgsl/kgsl-3d0/default_pwrlevel
@done
core=true
root=true
!view
!module
