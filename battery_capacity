!module
class=extra
!view
view=title
text=Modify current battery level
@show
if [ -e /sys/class/power_supply/battery/capacity ]
then
echo true
fi
@show
core=true

!view

!view
view=subtitle
text=Only applicable on some Qualcomm devices
@show
echo true
@show
!view

!view
view=edittext
text=Current battery level
@current
cat /sys/class/power_supply/battery/capacity
@current
@show
if [ -e /sys/class/power_supply/battery/capacity ]
then
echo true
fi
@show
@done
echo $edittext > /sys/class/power_supply/battery/capacity
@done
root=true
!view

!view
view=button
text=Restore
@show
echo true
@show
@click
echo -1 > /sys/class/power_supply/battery/capacity
@click
root=true
!view
!module