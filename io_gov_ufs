!module
class=io

!view
view=title
text=(Storage I/O) I/O Scheduler
@show
echo true
@show
!view

!view
view=subtitle
text=(UFS)Configure I/O Scheduler
@show
echo true
@show
!view

!view
view=spinner
text=Scheduler
@show
if [ -e /sys/block/sda/queue/scheduler ]
then
echo true
fi
@show
core=true
item_file_io=/sys/block/sda/queue/scheduler
@done
echo $label > /sys/block/sda/queue/scheduler
echo @toast=Successfully applied.
@done
@current
echo "io_label=`cat /sys/block/sda/queue/scheduler`"
@current
root=true
onboot=true
!view


!module
