!module
class=extra
!view
view=title
text=Lock screen password clear
@show
echo true
@show
!view

!view
view=subtitle
text=Clear lock screen password but keep fingerprint (may cause system interface infinite crash on some devices!!)
@show
echo true
@show
!view

!view
view=button
text=Clean!
@show
echo true
@show
core=true
root=true
@click
rm -f /data/system/gatekeeper.password.key
rm -f /data/system/gatekeeper.pattern.key
echo @toast=Done!
@click
!view
!module