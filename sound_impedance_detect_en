!module
class=extra
!view
view=title
text=Inpedance Detect
@show
echo true
@show
!view

!view
view=subtitle
text=Seemed able to improve the sound quality of a high-inpedance headset.
@show
echo true
@show
!view

!view
view=switch
text=Enable
@show
if [ -e /sys/module/snd_soc_wcd9xxx/parameters/impedance_detect_en ]
then
echo true
fi
@show
root=true
core=true
@click
echo $switch > /sys/module/snd_soc_wcd9xxx/parameters/impedance_detect_en
@click
@current
if [ `cat /sys/module/snd_soc_wcd9xxx/parameters/impedance_detect_en` == 1 ]
then
echo true
fi
@current

!view

!module
