!module
class=io
!view
view=title
text=(Storage I/O) I/O Statistics
@show
echo true
@show
!view

!view
view=subtitle
text=(EMMC)Boost performance if disabled, but apps rely on this may do not work normally.
@show
echo true
@show
!view

!view
view=switch
text=Enable
@show
if [ -e /sys/block/mmcblk0/queue/iostats ]
then
echo true
fi
@show
core=true
@current
if [ `cat /sys/block/mmcblk0/queue/iostats` == 1 ]
then
echo true
fi
@current
@click
echo $switch > /sys/block/mmcblk0/queue/iostats
@click
root=true
!view
!module
