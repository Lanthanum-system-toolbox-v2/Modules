!module
class=cpua
!view
view=title
text=cpuset job-allocating method
@show
echo true
@show
!view
!view
view=subtitle
text=configure how to allocate cores of processor
@show
echo true
@show
!view

!view
view=edittext
text=Background jobs：
root=true
@show
if [ -e /dev/cpuset/background/cpus ]
then
echo true
fi
@show
core=true
@current
cat /dev/cpuset/background/cpus
@current
@done
chmod 644 /dev/cpuset/background/cpus
echo $edittext > /dev/cpuset/background/cpus
@done
!view

!view
view=edittext
text=foreground jobs：
root=true
