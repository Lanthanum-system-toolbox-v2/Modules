!module
class=cpua
!view
view=title
text=EAS Schetune Boost
@show
echo true
@show
!view
!view
view=subtitle
text=Kernel-level EAS Scheduler(-100 to 100)(More radical if bigger)
@show
echo true
@show
!view

!view
view=edittext
text=Background jobs：
root=true
@show
if [ -e /dev/stune/background/schedtune.boost ]
then
echo true
fi
@show
core=true
@current
cat /dev/stune/background/schedtune.boost
@current
@done
chmod 644 /dev/stune/background/schedtune.boost
echo $edittext > /dev/stune/background/schedtune.boost
@done
!view

