!module
class=io
!view
view=title
text=RQ Affinity (Storage I/O)
@show
echo true
@show
!view

!view
view=subtitle
text=(EMMC)Control how radical the cpu is when encounter I/O event
@show
echo true
@show
!view

!view
view=seekbar
text=《——more conservative\s\s\s\s\s\s\s\s\s\s\s\s\s\s\s\s\smore radical——》
@show
if [ -e /sys/block/mmcblk0/queue/rq_affinity ]
then
echo true
fi
@show
core=true
root=true

@max
echo 2
@max

@current
cat /sys/block/mmcblk0/queue/rq_affinity
@current

@done
echo $seekbar > /sys/block/mmcblk0/queue/rq_affinity
@done
!view

!module
